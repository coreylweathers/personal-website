# Development config
# Add a contact form ID for the contact us form to work during local development by copying this file and renaming to params.toml, then update the value below.
contact_form_id = ""

# Development-only settings
debug_mode = true
show_drafts = true

# Analytics config - disabled in development
[analytics]
  [analytics.google]
    id = ""  # Empty in development to disable tracking
    respectDoNotTrack = true
  
  [analytics.clarity]
    id = ""  # Empty in development to disable tracking